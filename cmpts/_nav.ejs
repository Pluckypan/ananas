<header>
	<div class="inner detail-header clearfix">
		<a class="logo-box" href="<%=relative_path%>index.html">
			<img data-src="<%=relative_path%>images/logo.png|<%=relative_path%>images/logo@2x.png" src="<%=relative_path%>images/logo.png"/>
		</a>
		<a href="<%=relative_path%>/sponsor" class="sponsor-box">
			<img data-src="<%=relative_path%>images/icon.png|<%=relative_path%>images/icon.png" src="<%=relative_path%>images/icon.png"/>
		</a>
		<ul class="nav">
			<li class="col <%='index'==type?'active':''%>"><a href="<%=relative_path%>index.html">首页</a></li>
			<li class="col <%='trends'==type?'active':''%>"><a href="<%=relative_path%>trends.html">排行</a></li>
			<li class="col <%='articles'==type?'active':''%>"><a href="<%=relative_path%>articles.html">文章</a></li>
			<li class="col <%='apps'==type?'active':''%>"><a href="<%=relative_path%>apps.html">应用</a></li>
			<li class="col <%='videos'==type?'active':''%>"><a href="<%=relative_path%>videos.html">视频</a></li>
			<% if(type != 'search'){ %>
				<li class="last-col">
					<div class="quick-search"><span>快速搜索</span><i class="icon-search"></i></div>
				</li>
			<%}%>
		</ul>
		<% if(type != 'search'){ %>
			<%- include('_search_top',{type: type}); %>
		<%}%>
	</div>
	<div class="search-mask"></div>
	<% if( type == 'videos'){ %> 
		<%- include('_tags_video',{type: type}); %>
	<%}%>
</header>